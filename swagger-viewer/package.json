{
  "name": "swagger-viewer",
  "version": "1.2.0",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "scripts": {
    "build": "webextension-toolbox build chrome",
    "dev": "webextension-toolbox dev chrome",
    "eslint-print-config": "eslint --print-config src/index.tsx > misc/eslint-print-config.json",
    "lint-dry": "npm-run-all --parallel --continue-on-error lint:eslint-dry lint:tsc",
    "lint:eslint-dry": "eslint -c .eslintrc.js --ext .js,.ts,.tsx .",
    "lint:eslint": "eslint -c .eslintrc.js --fix --ext .js,.ts,.tsx .",
    "lint:tsc": "tsc --noEmit",
    "lint": "npm-run-all --parallel --continue-on-error lint:eslint lint:tsc",
    "test-ci": "jest --ci",
    "test": "jest --watch"
  },
  "devDependencies": {
    "@types/chrome": "0.0.103",
    "@types/jest": "25.2.1",
    "@types/js-yaml": "3.12.3",
    "@types/jsdom": "16.2.0",
    "@types/node": "13.11.0",
    "@types/react": "16.9.32",
    "@types/react-dom": "16.9.6",
    "@types/react-test-renderer": "16.9.2",
    "@types/styled-components": "5.0.1",
    "@types/webpack": "4.41.10",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "babel-jest": "25.2.6",
    "babel-preset-react-app": "9.1.2",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-config-react-app": "5.2.1",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-only-error": "1.0.2",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.0",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-typescript-sort-keys": "0.8.0",
    "import-sort-style-module": "6.0.0",
    "jest": "25.2.7",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.2",
    "prettier-plugin-import-sort": "0.0.4",
    "regenerator-runtime": "0.13.5",
    "style-loader": "1.1.3",
    "terser-webpack-plugin": "3.0.3",
    "typescript": "3.8.3",
    "typescript-styled-plugin": "0.15.0",
    "webextension-toolbox": "3.0.0"
  },
  "dependencies": {
    "buffer": "5.5.0",
    "jest-styled-components": "7.0.0",
    "js-yaml": "3.13.1",
    "jsdom": "16.2.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-test-renderer": "16.13.1",
    "styled-components": "5.0.1",
    "swagger-ui-react": "3.25.0"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
